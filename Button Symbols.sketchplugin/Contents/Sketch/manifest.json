{
    "name": "Button Symbols",
    "description": "A plugin to make buttons easier to customize in Sketch (for Sketch 46.2+)",
    "author": "Michael White",
    "copyright": "2017 Michael White",
    "homepage": "https://github.com/mwhite05/Button-Symbols",
    "version": "1.4.0",
    "identifier": "io.freckle.sketch.button-symbols",
    "appcast": "https://api.sketchpacks.com/v1/plugins/io.freckle.sketch.button-symbols/appcast",
    "compatibleVersion": "46.2",
    "commands": [
        {
            "script": "command-handlers.js",
            "shortcut": "cmd j",
            "name": "Update Selected",
            "identifier": "buttonsymbols_update",
            "handler": "command_updateSelectedButtons"
        },
        {
            "script": "command-handlers.js",
            "shortcut": "ctrl cmd shift b",
            "name": "Convert Selection to Button",
            "identifier": "buttonsymbols_convert",
            "handler": "command_convertButtonSymbolForSelection"
        },
        {
            "script": "command-handlers.js",
            "name": "Update within Selected Groups",
            "identifier": "buttonsymbols_updateInGroups",
            "handler": "command_updateButtonsInSelectedGroups"
        },
        {
            "script": "command-handlers.js",
            "name": "Update within Selected Artboards",
            "identifier": "buttonsymbols_updateInArtboards",
            "handler": "command_updateButtonsInSelectedArtboards"
        },
        {
            "script": "command-handlers.js",
            "name": "Update within Selected Pages",
            "identifier": "buttonsymbols_updateInPages",
            "handler": "command_updateButtonsInSelectedPages"
        },
        {
            "script": "command-handlers.js",
            "name": "Update All Buttons",
            "identifier": "buttonsymbols_updateAllButtons",
            "handler": "command_updateAllButtons"
        },
        {
            "script": "action-handlers.js",
            "handlers": {
                "actions": {}
            }
        }
    ],
    "menu": {
        "isRoot": false,
        "items": [
            "buttonsymbols_update",
            "buttonsymbols_convert",
            "buttonsymbols_updateInGroups",
            "buttonsymbols_updateInArtboards",
            "buttonsymbols_updateInPages",
            "buttonsymbols_updateAllButtons"
        ],
        "title": "Button Symbols"
    }
}
