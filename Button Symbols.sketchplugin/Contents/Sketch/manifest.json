{
    "name": "Button Symbols",
    "description": "A plugin to make buttons easier to customize in Sketch (for Sketch 43.1+)",
    "author": "Freckle",
    "homepage": "https://github.com/mwhite05/Button-Symbols",
    "version": "1.0.0",
    "identifier": "io.freckle.sketch.button-symbols",
    "compatibleVersion": "43.1",
    "bundleVersion": "1.0.0",
    "commands": [
        {
            "script": "plugin.js",
            "shortcut": "cmd j",
            "name": "Button Symbols",
            "identifier": "buttonsymbols",
            "handler": "command_updateButtonSymbols"
        },
        {
            "script": "plugin.js",
            "handlers": {
                "actions": {
                    "SelectionChanged.finish": "onSelectionChanged",
                    "TextChanged.finish": "onTextChanged"
                }
            }
        }
    ],
    "menu": {
        "isRoot": false,
        "items": [
            "buttonsymbols",
            "fixsymbolnames"
        ],
        "title": "Button Symbols"
    }
}
